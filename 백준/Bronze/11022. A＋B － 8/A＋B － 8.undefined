#include <bits/stdc++.h>

#include <format>

using namespace std;

int main() {
  ios::sync_with_stdio(false);
  cin.tie(nullptr);

  int t;
  cin >> t;
  for (int i = 1; i <= t; i++) {
    int64_t a, b;
    cin >> a >> b;
    cout << format("Case #{}: {} + {} = {}\n", i, a, b, a + b);
  }

  return 0;
}
